/*下拉菜单  */
 
/* 下面是tobar */
 page{background-color: #fcfcfc;}
#swiper-tab {
  width: 100%;
  position: relative;
}
 
.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 65rpx;
  background-color: white;
}
 
.swiper-tab-list {
  font-size: 25rpx;
  display: inline-block;
  border-top:1px  #a8a5a5 solid;
  border-bottom:1px  #a8a5a5 solid;

  width:25%;
  color: #666;
  position: relative;
}

.swiper-tab-list i {
  position: absolute;
  bottom: 30%;
  left:14%;
  width: 0px;
  height: 0px;
  border: 5px solid  #a8a5a5;
  border-left-color:   #a8a5a5;
  border-bottom-color: transparent;
  border-top-color: transparent;
  border-right-color: transparent;
}

.on {
  color: #0e0d01;
  border-bottom: 2rpx solid #0e0d01;
}
 
.on i {
  border: 5px solid #0e0d01;
  border-top-color: #0e0d01;
  border-bottom-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}
 
.swiper-box view {
  text-align: center;
}
 
.swiper-item {
  font-size: 25rpx;
  background: #fff;
  width: 100%;
  height: 600rpx;
  display: none;
  position:absolute;
  top: 68rpx;
  z-index: 333333;
  left:0;
  animation: displays 1s;
}
 
@keyframes displays {
  from {
    height: 0px;
  }
 
  to {
    height: 600rpx;
  }
}
 
.swiper-beijing {
  width: 100%;
  height: 200%;
  opacity: 0.5;
  position: absolute;
  top: 130px;
  left: 0;
  display: none;
  z-index: 888;
  animation: 1s;
}
 
 
